.form-group
  label Seleccione su archivo Excel con los stocks
  input.form-control(type="file", ng-file-select="", on-read-file="parseExcel($fileContent)")

.row
  form.form-horizontal
    .form-group.col-sm-4(ng-repeat="columna in columnasExcelRequeridas")
      label.control-label {{columna}}
      select.form-control(ng-model='settings.columns[columna]', ng-options='it for it in columnasExcel')

pre {{ primeraFilaExcel | json }}

button.btn.btn-info(ui-sref='settings.colores') Continuar

